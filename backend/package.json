{
  "name": "agenda-backend",
  "version": "1.0.1",
  "description": "Agenda de contatos",
  "main": "server.ts",
  "author": "Iago Colodetti",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=env.development ts-node-dev --respawn --transpile-only src/server.ts",
    "clean": "rimraf dist",
    "copyfiles": "copyfiles -a ./tests/database/agenda.sqlite ./config/* ./src/configs/swagger.yaml ./dist",
    "build": "yarn clean && tsc && yarn copyfiles",
    "start": "cross-env NODE_ENV=env.development node ./dist/src/server.js",
    "test:ts": "cross-env NODE_ENV=env.test jest --verbose --watchAll .*.spec.ts",
    "test:js": "cross-env NODE_ENV=env.test jest --verbose .*.spec.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.3.5",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.0",
    "sequelize-typescript": "^2.1.5",
    "swagger-ui-express": "^4.6.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.17",
    "@types/yamljs": "^0.2.31",
    "copyfiles": "^2.4.1",
    "jest": "^29.5.0",
    "rimraf": "^5.0.1",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  }
}
